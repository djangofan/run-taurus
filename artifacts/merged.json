{
 "scenarios": {
  "testit-jmx": {
   "script": "testit.jmx"
  }
 }, 
 "execution": [
  {
   "ramp-up": "30s", 
   "scenario": "testit-jmx", 
   "concurrency": 20
  }
 ], 
 "modules": {
  "jmeter": {
   "gui": false
  }, 
  "consolidator": {
   "max-buffer-len": "2h", 
   "buffer-multiplier": 2, 
   "ignore-labels": [
    "ignore"
   ], 
   "min-buffer-len": "2s", 
   "percentiles": [
    0, 
    50, 
    90, 
    95, 
    99, 
    99.9, 
    100
   ], 
   "rtimes-len": 500, 
   "buffer-scale-choice": 0.5, 
   "generalize-labels": false
  }
 }, 
 "reporting": [
  {
   "module": "blazemeter"
  }, 
  {
   "dump-csv": "final-stats.csv", 
   "module": "final-stats"
  }, 
  {
   "module": "console"
  }, 
  {
   "module": "junit-xml"
  }
 ]
}
